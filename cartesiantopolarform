#include <stdio.h>
#include <math.h>
int main()
{

  float x,y,ht,theta;
  printf("Enter x coordinates\n");
  scanf("%f",&x);
  printf("Enter y coordinates \n");
  scanf("%f",&y);
  
  ht = sqrt(x*x + y*y);
  printf("%f \n",ht);
  
  theta = pow(tan(x/y),-1);

    printf("In polar form %f %f\n",ht,theta);
    return 0;
}
