// Online C compiler to run C program online
#include <stdio.h>

int main() {
    printf ("Enter the number you want to find table of\n");
    int a;
    scanf ("%d" , &a);
    int multiply(int a ,int b){
        return a*b;
    }
    for (int i=1;i < 11 ;i++){
        printf ("%d x %d = %d\n", a,i,multiply(a,i));
    }
    return 0;
}
