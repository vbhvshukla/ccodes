#include <stdio.h>

int size(int length,int breadth){
    int templ,tempb;
    templ = breadth;
    tempb = length/2;
    printf("New length and new breadth are as %d,%d",templ,tempb);
    }
int main()
{
    int olength = 1189;
    int obreadth=841;
    
    size(olength,obreadth);
}
